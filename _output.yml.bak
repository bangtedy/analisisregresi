bookdown::bs4_book:
  css: css/style.css
  theme:
    primary: "#101aa3"
  repo: https://github.com/bangtedy/analisisregresi
  includes:
    in_header: [ga_script.html, material_icons.html]
bookdown::gitbook:
  css: style.css
  config:
    config:
    sharing:
        facebook: yes
        twitter: yes
        linkedin: yes
        google: no
        weibo: no
        instapper: no
        vk: no
        all: ['facebook', 'twitter', 'linkedin', 'google', 'weibo', 'instapaper']
    toc:
      before: |
        <li><a href="./">Analisis Regresi <br> Tedy Herlambang</a></li>
      after: |
        <li><a href="https://github.com/rstudio/bookdown" target="blank">Published with bookdown</a></li>
    edit: https://github.com/rstudio/bookdown-demo/edit/master/%s
    download: ["pdf", "epub"]
bookdown::pdf_book:
  includes:
    in_header: preamble.tex
  latex_engine: xelatex
  citation_package: natbib
  keep_tex: yes
bookdown::epub_book: default

